class Solution {
public:
    char findTheDifference(string s, string t) {
        sort(s.begin(), s.end());
        sort(t.begin(), t.end());
        char ans = t[t.size()-1];
        for(int i = 0; i<t.size() and i<s.size(); ++i){
            if(s[i]!=t[i]){
                return t[i];
            }
        }
        return ans;
    }
};
